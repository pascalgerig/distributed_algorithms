# Change the path of etcd if etcd is located elsewhere

n1: ./etcd --name node1 --listen-client-urls http://0.0.0.0:10001 --advertise-client-urls http://0.0.0.0:10001 --listen-peer-urls http://127.0.0.1:11001 --initial-advertise-peer-urls http://127.0.0.1:11001 --initial-cluster-token etcd-cluster-1 --initial-cluster 'node1=http://127.0.0.1:11001,node2=http://127.0.0.1:11002,node3=http://127.0.0.1:11003,node4=http://127.0.0.1:11004,node5=http://127.0.0.1:11005' --initial-cluster-state new --enable-pprof --logger=zap --log-outputs=stderr --log-level=warn

